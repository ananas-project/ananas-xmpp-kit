<AXK xmlns="ananas:axk:base" xmlns:kit1="ananas:axk:ex:kit1">
	<Connection>
		<FilterList>

			<Local />
			<kit1:Terminal>
				<Import url="resource:///ananas/axk2/terminal/commands.xml" />
			</kit1:Terminal>
			<kit1:Monitor />
			<kit1:ContactModel />

			<Switch>
				<Case namespace="jabber:client">
					<FilterList>
						<kit1:MessageManager />
						<kit1:PresenceManager />
						<kit1:RosterManager />
						<kit1:SelfManager />
					</FilterList>
				</Case>

				<Case>
					<!-- default -->
					<FilterLoader class="ananas.axk2.core.bp.target.DefaultNSFilter" />
				</Case>

			</Switch>


			<Engine />
			<Remote />

		</FilterList>
	</Connection>
</AXK>
